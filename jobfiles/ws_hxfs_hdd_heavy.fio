[global]
direct=1
ioengine=libaio
random_generator=tausworthe64
size=100%
loops=10000
invalidate=1
time_based
norandommap
randrepeat=0
ramp_time=${RAMPTIME}
runtime=${TIME}
group_reporting=1

[job0]
rw=randrw
rwmixread=90
percentage_random=20,47,
numjobs=${DEPTH1}
rate=${RATE1}
bssplit=${BLKSIZE}
filename=${DEV}
