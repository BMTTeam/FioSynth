[global]
rw=randrw
blocksize=${BLKSIZE}
blockalign=${BLKSIZE}
size=100%
ioengine=libaio
iodepth=${DEPTH}
rwmixread=70
rwmixwrite=30
direct=1
loops=10000
invalidate=1
time_based
ramp_time=${RAMPTIME}
norandommap
randrepeat=0
runtime=${TIME}
group_reporting=1
random_generator=tausworthe64
numjobs=${JOBS}
[job0]
filename=${DEV}
